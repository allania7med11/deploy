# use the container to generate structure_front and copy it to /var/www/
sudo cp -r portfolio /var/www/
# copy ngnix portfolio config and enable it
sudo cp /vagrant/ngnix/sites-available/portfolio /etc/nginx/sites-available
sudo ln -s /etc/nginx/sites-available/portfolio /etc/nginx/sites-enabled/
# restart nginx
sudo nginx -t
sudo systemctl restart nginx